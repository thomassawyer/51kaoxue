    <script src="~/CustomScripts/Elite_School.js"></script>
    <script src="~/CustomScripts/jquery.reveal.js"></script>
</head>
<body>

    @Html.Partial("~/Common/top2.ascx")
    <div class="xueduan">
        <div class="xd5 fl">
            <div class="beijing">
                <div class="xd5_div1 fl"><img src="img/duanluo.png" class="xdtb5"><a target='_blank' target='_blank' class="xd5a"><b>学 段</b></a></div>
                <div class="xd5_hover fl xd5_hover_selected" id="level3" onclick="a_selected(this, 'xd5_hover_selected'), level_selected(3)">
                    <a target='_blank' target='_blank'>高中</a>
                </div>
                <div class="xd5_hover fl" id="level2" onclick="a_selected(this, 'xd5_hover_selected'), level_selected(2) ">
                    <a target='_blank' target='_blank'>初中</a>
                </div>

            </div>
        </div>
        <div class="xd5 fl">
            <div class="xd5_div1 fl bg_07c277">
                <a target='_blank' target='_blank' class="xd5a"><img src="img/xueke.png" class="xdtb5"><b>年 级</b></a>
            </div>
            <div class="xd522 fl" style="height:100%;">
                <div class="xdl1039 fl" id="grade">

                </div>
            </div>

        </div>
        <div class="xueduan150 fl" style="margin-bottom:20px;">
            <div class="xd150 fl">
                <a target='_blank' target='_blank' class="mxdq"><b>地 区</b></a>
                <img src="img/leixin.png" class="img150xd">
            </div>
            <div class="xuduan33" style="height:100%;">
                <div class="xdl1039 fl" id="district">
                </div>
            </div>
        </div>
    </div><!-- 学段 -->

    <div class="lianxi" style="overflow:hidden;">
        <div class="lianxice210 fl">
            <div class="lianxice33">
                <span class="xzgd1">更多<img src="img/raw_d.png" class=""></span>
                <img src="img/xiazaitubiao.png" class="xztubiao p_ab_t7">
                <a target='_blank' target ='_blank'>热门下载</a>
            </div>
            <div class="lxgao521 mar_top_z10 border_lr_e5 border_bott_e5">
                <ul class="remenxiazai1" id="hot_download"></ul>
            </div>
            <div class="lianxice33 rmxz333 ma20">
                <span class="xzgd1">更多<img src="img/raw_d.png" class=""></span>
                <img src="img/tuijianxg.png" class="xztubiao">
                <a target='_blank' target='_blank'>相关推荐</a>
            </div>
            <div class="lxgao521 mar_top_z10 border_lr_e5 border_bott_e5">
                <ul class="remenxiazai1" id="recommend"></ul>
            </div>
        </div>
        <div class="lianxice fl mar_lf_18">
            <div class="lxclan">
                <img src="img/doc_bag.jpg" class="doc_bag_p_ab"><a target='_blank' target='_blank' class="bix">文件</a>
            </div>
            <div class="" id="data_list_td">
            </div>
            <div class="pager" id="pages">
            </div> <!-- 跳转部分 -->
        </div>
        </div>

    <script>
        var shuzhi1 = $(".ysp1").html();
        var zs1 = Number(shuzhi1);

        var shuzhi2 = $(".ysp2").html();
        var zs2 = Number(shuzhi2);

        var shuzhi3 = $(".ysp3").html();
        var zs3 = Number(shuzhi3);

        var shuzhi4 = $(".ysp4").html();
        var zs4 = Number(shuzhi4);

        var shuzhi5 = $(".ysp5").html();
        var zs5 = Number(shuzhi5);

        $(".xyy1").click(function () {
            var jd1 = zs1 += 1;
            var jd2 = zs2 += 1;
            var jd3 = zs3 += 1;
            var jd4 = zs4 += 1;
            var jd5 = zs5 += 1;
            $(".ysp1").html(jd1);
            $(".ysp2").html(jd2);
            $(".ysp3").html(jd3);
            $(".ysp4").html(jd4);
            $(".ysp5").html(jd5);
        });

        $(".syy1").click(function () {
            var jd1 = zs1 - 1;
            var jd2 = zs2 - 1;
            var jd3 = zs3 - 1;
            var jd4 = zs4 - 1;
            var jd5 = zs5 - 1;
            $(".ysp1").html(jd1);
            $(".ysp2").html(jd2);
            $(".ysp3").html(jd3);
            $(".ysp4").html(jd4);
            $(".ysp5").html(jd5);


        });
        //页面分页加载

        function go() {
            var zansan = $("#lll").val()
            var goyemian = Number(zansan);
            $(".ysp1").html(goyemian);
            $(".ysp2").html(goyemian + 1);
            $(".ysp3").html(goyemian + 2);
            $(".ysp4").html(goyemian + 3);
            $(".ysp5").html(goyemian + 4);
        }
        //go改变页码
    </script>

    @Html.Partial("~/Common/bottom.ascx")
</body>
</html>