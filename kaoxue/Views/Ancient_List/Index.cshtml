@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>好资源 - 文学国学分类</title>
    <link href="~/CSS/common.css" rel="stylesheet" />
    <link href="~/CSS/Beike_Center.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="css/default.css" />

    <!-- Edit Below -->
    <link rel="stylesheet" type="text/css" href="css/style.css" />
    <link href="~/CSS/Test_Center.css" rel="stylesheet" />
    <link href="~/CSS/Ancient.css" rel="stylesheet" />
    <link href="~/CSS/Ancient_List.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.8.2.js"></script>
    @* 弹窗样式表 *@
    <link rel="stylesheet" href="../css/reveal.css">
    @* 弹窗演示表结束 *@
    @* 弹窗脚本 *@
    <script type="text/javascript" src="../CustomScripts/jquery.reveal.js"></script>
    @* 弹窗脚本结束 *@
    <script src="~/CustomScripts/Ancient_List.js"></script>
</head>
<body>
    <div id="Main">
        @Html.Partial("~/Common/top3.ascx")
        <div id="container">
            <div id="content">
                <div id="nav_tree">
                    <span id="tree_left">
                        首页 > <a href="../Ancient">文学国学</a>
</span>
                    <span id="tree_right">
                        @Html.Partial("~/Common/Search2.ascx")
                    </span>
                </div>
            </div>
            <div id="category_content">

                <div id="category_content_left">
                    <div class="page_title" id="page_title">
                        
                    </div>
                    @* 数据列表开始 *@
                    <div class="data_list" id="lists" style="min-height:1100px;">
                        <div class="data_list_td_container">
                            <div class="data_list_td_container_left">
                                <img src="../Images/%e5%a4%87%e8%af%be%e4%b8%ad%e5%bf%83/%e6%96%87%e6%a1%a3.png">
                            </div>
                            <div class="data_list_td_container_middle" style="width:500px">
                                <div>
                                    <span id="text_title">河北省武邑中学2017届高三上学期周考（8.21）政治试题（扫描版）</span>
                                </div>
                            </div>
                        </div>
                    </div>
                    @* 数据列表结束 *@
                    <div id="pager" style="float:left;">
                        <div id="pager_buttons">
                            <div id="prev">
                                <a onclick="anchor(this),pre_page() ">上一页</a>
                            </div>
                            <div id="pages">

                            </div>
                            <div id="next"><a onclick="anchor(this),next_page() ">下一页</a></div>
                            <div id="turn"><span>跳转到</span><input type="text" id="page_size" /><span>页</span><a class="pages_href_selected" onclick="anchor(this),Go() ">GO</a></div>
                        </div>
                    </div>
                </div>
                <div id="category_content_right">
                    @* 阅读排行榜 *@
                    <div id="Reading_Lists">
                        <div class="lists_title">
                            <span class="title_left">| 阅读排行榜</span>
                        </div>
                        <div id="Reading_List">
                        </div>
                    </div>

                    @* 相关推荐 *@
                    <div id="relative_recommends">
                        <div class="lists_title">
                            <span class="title_left">| 相关推荐</span>
                            <span class="title_right">More</span>
                        </div>
                        <div id="relative_recommend">
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
    @* 预览 *@
    <a href="#" id="message_preview" class="big-link" data-reveal-id="myModal_preview" style="display:none;">
        jquery1
    </a>

    <div id="myModal_preview" class="reveal-modal" style="border: 1px solid #3b4f7c;width:900px; margin-left:-440px; padding:0px; ">
        <iframe id="download_preview" width="895" height="500"></iframe>
        <a class="close-reveal-modal" id="message_close">&#215;</a>
    </div>


    @* 消息提示 *@
    <a href="#" id="message_download" class="big-link" data-reveal-id="myModal_download" style="display:none;">
        jquery1
    </a>
    <div id="myModal_download" class="reveal-modal" style="border: 1px solid #3b4f7c; ">
        <h1 style="font-family:'幼圆'" id="message_content">系统提示:</h1>
        <p id="message_text" style="font-family:'微软雅黑'">Attention please.</p>
        <a class="close-reveal-modal" id="message_close">&#215;</a>
    </div>
    @Html.Partial("~/Common/bottom.ascx")
</body>
</html>

